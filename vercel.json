{
  "version": 2,
  "buildCommand": "vite build",
  "rewrites": [
    {
      "source": "/api/probe",
      "destination": "/api/probe"
    },
    {
      "source": "/api/debug",
      "destination": "/api/debug"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    }
  ]
}
